pipeline {
  agent { label "linux" }
  stages {
    stage("build") {
      steps {
        sh """
          cd Trabalho52;
          docker build -t Trabalho52 .
        """
      }
    }
    stage("run") {
      steps {
        sh """
          docker run --rm Trabalho52
        """
      }
    }
  }
}


